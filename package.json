{
  "name": "moment-cache",
  "description": "Simple utility to cache moment.js instances.",
  "author": "Artem Yavorsky",
  "version": "0.1.2",
  "repository": {
    "type": "git",
    "url": "git@github.com:yavorsky/moment-cache.git"
  },
  "scripts": {
    "build": "babel ./lib --ignore moment --out-file moment-cache.js",
    "prepublish": "npm run build",
    "watch": "babel ./lib --watch --ignore moment --out-file moment-cache.js"
  },
  "main": "./moment-cache.js",
  "dependencies": {
    "moment": "~2.15.0"
  },
  "keywords": [
    "moment",
    "moment-js",
    "time",
    "date",
    "datetime",
    "cache",
    "speed",
    "performance",
    "chicks"
  ],
  "devDependencies": {
    "browserify": "^13.0.0",
    "babelify": "^7.2.0",
    "babel-cli": "^6.9.0",
    "babel-preset-es2015": "6.6.0"
  },
  "babel": {
    "presets": ["es2015"]
  }
}
